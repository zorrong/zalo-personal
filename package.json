{
  "name": "zalo-personal",
  "version": "1.3.1",
  "description": "OpenClaw extension for Zalo Personal Account via zca-js library. Login via QR code, no CLI tools needed. Works with OpenClaw 2026.2+",
  "type": "module",
  "main": "./index.ts",
  "author": "caochitam <caochitam@gmail.com>",
  "license": "MIT",
  "keywords": [
    "openclaw",
    "openclaw-extension",
    "zalo",
    "zalo-personal",
    "vietnam",
    "messaging",
    "chatbot",
    "qr-login"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caochitam/zalo-personal.git"
  },
  "bugs": {
    "url": "https://github.com/caochitam/zalo-personal/issues"
  },
  "homepage": "https://github.com/caochitam/zalo-personal#readme",
  "files": [
    "src/",
    "script/",
    "README.md",
    "INSTALL.md",
    "QUICK-REFERENCE.vi.md",
    "LICENSE",
    "quick-install.sh",
    "config-helper.cjs",
    "openclaw.plugin.json",
    "index.ts",
    "zalo-group.jpg"
  ],
  "scripts": {
    "prepublishOnly": "echo 'Ready to publish!'"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.0"
  },
  "dependencies": {
    "@sinclair/typebox": "0.34.48",
    "jsqr": "^1.4.0",
    "pngjs": "^7.0.0",
    "qrcode-terminal": "^0.12.0",
    "zca-js": "^2.0.4",
    "zod": "^4.3.6"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "zalo-personal",
      "label": "Zalo Personal",
      "selectionLabel": "Zalo Personal Account",
      "docsPath": "/channels/zalo-personal",
      "docsLabel": "zalo-personal",
      "blurb": "Zalo personal account via zca-js library (no CLI needed).",
      "aliases": [
        "zp"
      ],
      "order": 86,
      "quickstartAllowFrom": true
    },
    "install": {
      "localPath": "extensions/zalo-personal",
      "defaultChoice": "local"
    }
  }
}
